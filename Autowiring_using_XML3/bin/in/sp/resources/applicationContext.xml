<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean class="in.sp.beans.Address" id="address1">
		<constructor-arg name="houseno" value="121"></constructor-arg>
		<constructor-arg name="city" value="Patna"></constructor-arg>
		<constructor-arg name="pincode" value="789456"></constructor-arg>
	</bean>
	
	
	<bean class="in.sp.beans.Student" id="std" autowire="constructor">
		<constructor-arg name="roll" value="123"></constructor-arg>
		<constructor-arg name="name" value="Seeta"></constructor-arg>
		
	</bean>

</beans>