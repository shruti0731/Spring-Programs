<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
        
	<context:annotation-config />

    <bean class="in.sp.beans.Address" id = "addr">
		<property name="houseno" value="12300"></property>
		<property name="city" value="Sangli"></property>
		<property name="pincode" value="416416"></property>
	</bean>
	
	<bean class="in.sp.beans.Student" id="stdbean">
		<property name="name" value="Radhika" />
		<property name="roll" value="11" />
	</bean>
	
</beans>